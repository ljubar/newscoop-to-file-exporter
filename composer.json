{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.0",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ahs/ninjs-php": "dev-master",
        "ahs/superdesk-publisher-sdk": "dev-master",
        "behat/transliterator": "^1.2",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/annotations": "^1.0",
        "doctrine/common": "^2",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^2",
        "doctrine/orm": "^2",
        "guzzlehttp/guzzle": "^6.3",
        "hoa/mime": "3.17.01.10",
        "php-amqplib/rabbitmq-bundle": "^1.14",
        "phpdocumentor/reflection-docblock": "^5.1",
        "sensio/framework-extra-bundle": "^5.2",
        "swp/bridge": "1.4.*-dev",
        "symfony/console": "^3.3",
        "symfony/dotenv": "^3.3",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/property-access": "^3.3",
        "symfony/property-info": "^3.3",
        "symfony/serializer": "^4.3",
        "symfony/yaml": "^3.3",
        "thecodingmachine/safe": "^0.1.13"
    },
    "require-dev": {
        "deployer/deployer": "^6.0",
        "phpstan/phpstan": "^0.11.2",
        "phpstan/phpstan-symfony": "^0.11.1",
        "symfony/stopwatch": "^3.3",
        "symfony/web-profiler-bundle": "^3.3",
        "thecodingmachine/phpstan-safe-rule": "^0.1.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BWPZR8Y0SCV0RAZMQ1W10G1M",
            "allow-contrib": true
        }
    }
}
